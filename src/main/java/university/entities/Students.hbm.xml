<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="university.entities.Students" table="students" schema="university">
        <id name="id" column="id"/>
        <property name="fio" column="fio"/>
        <property name="group" column="group"/>
        <property name="scolarship" column="scolarship"/>
        <property name="dateEnter" column="date_enter"/>
    </class>
</hibernate-mapping>